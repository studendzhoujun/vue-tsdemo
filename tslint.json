{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "quotemark":[false, "single","avoid-escape"], 
    "semicolon":[false, "never", "ignore-interfaces"], 
    "indent":[false, "tabs", 2], 
    "typedef-whitespace": false, 
    "whitespace": false,
    "member-access": false, 
    "one-line": false, 
    "trailing-comma": [false, { 
      "multiline": {
            "objects": "ignore",
            "arrays": "never",
            "functions": "never",
            "typeLiterals": "ignore"
        },
        "esSpecCompliant": true 
      }]
  }
}
